      "source": [
        "import spacy\n",
        "nlp = spacy.load('en_core_web_sm')"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "def pos(text):\n",
        "  doc = nlp(text)\n",
        "  print(doc.text)\n",
        "  for word in doc:\n",
        "    print(word.text,\"->\", word.pos_,word.tag_,spacy.explain(word.tag_))"
      ],
      "metadata": {
        "id": "bCWv7vF9EZWB"
      },
      "execution_count": 2,
      "outputs": []
    },
    {
